group "boundary_inner" add node in surface 27
nodeset 20 group boundary_inner
nodeset 20 name "boundary_inner"

# ----------------------------------------------------------------------
# Create nodeset for outer surface
# ----------------------------------------------------------------------
group "boundary_outer" add node in surface 29
nodeset 21 group boundary_outer
nodeset 21 name "boundary_outer

